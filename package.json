{
  "type": "module",
  "name": "inkdrop-version-history-beta",
  "version": "5.6.3-beta.0",
  "description": "The beta version history of Inkdrop for desktop",
  "main": "index.mjs",
  "scripts": {
    "build": "npm run build:json && npm run build:readme && npm run build:meta && npm run build:changelog",
    "build:readme": "node index.mjs",
    "build:json": "js-yaml history.yml > history.json",
    "build:meta": "node genmeta.mjs",
    "build:changelog": "node changelog.mjs",
    "release": "env $(cat .env | xargs) node release.mjs",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "inkdrop"
  ],
  "author": "Takuya Matsuyama <t@inkdrop.app>",
  "license": "MIT",
  "devDependencies": {
    "eslint": "^8.5.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.3.1"
  },
  "dependencies": {
    "dateformat": "^3.0.3",
    "js-yaml": "^4.1.0",
    "semver": "^7.5.4"
  }
}
